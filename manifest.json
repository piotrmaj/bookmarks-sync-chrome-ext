{
  "name": "Bookmarks Importer",
  "description": "A browser action which imports bookmarks from static json file to folder in bookmark bar",
  "version": "1.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "bg-wrapper.js"
  },
  "permissions": ["bookmarks", "notifications"],
  "action": {
    "default_title": "Bookmarks Importer",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
